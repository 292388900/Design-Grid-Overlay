{
  "manifest_version": 2,

  "name": "Grid Overlay",
  "description": "This extension creates an overlay for a grid system",
  "version": "0.1",
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
  "browser_action": {
    "default_icon": "icon32_off.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "tabs", "http://*/*"
  ]
}
